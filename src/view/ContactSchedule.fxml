<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ContactSchedule">
   <children>
      <VBox prefHeight="600.0" prefWidth="1000.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="96.0" prefWidth="333.0" spacing="10.0">
                     <children>
                        <Label text="Contact Schedule">
                           <font>
                              <Font size="28.0" />
                           </font>
                        </Label>
                        <Label layoutX="72.0" layoutY="41.0" text="View schedule for all contacts">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="413.0" prefWidth="1000.0">
               <children>
                  <TableView fx:id="contactTableView" prefHeight="413.0" prefWidth="951.0">
                    <columns>
                      <TableColumn fx:id="contactNameCol" prefWidth="104.0" text="Contact" />
                        <TableColumn fx:id="appointmentCol" prefWidth="104.0" text="Appointment ID" />
                      <TableColumn fx:id="titleCol" prefWidth="93.0" text="Title" />
                        <TableColumn fx:id="descriptionCol" prefWidth="124.0" text="Description" />
                        <TableColumn fx:id="dateCol" prefWidth="124.0" text="Appointment Date" />
                        <TableColumn fx:id="startCol" prefWidth="124.0" text="Start Time" />
                        <TableColumn fx:id="endCol" prefWidth="124.0" text="End Time" />
                        <TableColumn fx:id="customerIDCol" prefWidth="153.0" text="Customer ID" />
                    </columns>
                  </TableView>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="viewReportsBtn" mnemonicParsing="false" onAction="#onActionViewReports" text="Back To Report View">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>

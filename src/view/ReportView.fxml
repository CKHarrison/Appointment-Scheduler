<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="270.0" prefWidth="475.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportView">
   <children>
      <VBox prefHeight="270.0" prefWidth="475.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="69.0" prefWidth="475.0">
               <children>
                  <Label text="Reports">
                     <font>
                        <Font size="26.0" />
                     </font>
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="49.0" prefWidth="475.0">
               <children>
                  <Label text="Select which reports you want to view">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <Button fx:id="monthTypeButton" alignment="CENTER" mnemonicParsing="false" onAction="#onActionMonthAndType" prefHeight="64.0" prefWidth="128.0" text="By Month and Type" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="contactScheduleButton" alignment="CENTER" layoutX="184.0" layoutY="22.0" mnemonicParsing="false" onAction="#onActionContactSchedule" prefHeight="64.0" prefWidth="128.0" text="Contact Schedule" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="userScheduleBtn" alignment="CENTER" layoutX="248.0" layoutY="22.0" mnemonicParsing="false" onAction="#onActionUserSchedule" prefHeight="64.0" prefWidth="128.0" text="User Schedule" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onActionCancel" prefHeight="35.0" prefWidth="121.0" text="Main Menu">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
